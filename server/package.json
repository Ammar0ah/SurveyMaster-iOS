{
  "name": "surveymaster",
  "version": "1.0.0",
  "description": "Survey Manager server for creating and publishing surveys",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "yarn build && nodemon dist/server.js"
  },
  "author": "Mouaz Alkhodari",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.14.6",
    "rimraf": "^2.6.3",
    "yarn": "^1.15.2"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.2"
  }
}
